    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/react": "^16.3.2",
    "@types/express": "^5.0.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.13",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.5.12",
    "happy-dom": "^20.6.1",
    "postcss": "^8.0.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0"
  }
